{
  "$schema": "vscode://schemas/color-theme",
  "name": "IC10 Text Colors Only",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {},
  "semanticTokenColors": {
    "type": "#800080",
    "enum": "#20B2AA",
    "string": "#ff6516",
    "number": "#20B2AA",
    "macro": "#0080FF",
    "function": "#00FF00"
  },
  "tokenColors": [
    {
      "scope": ["entity.name.function.ic10"],
      "settings": { "foreground": "#FFFF00", "fontStyle": "" }
    },
    {
      "scope": ["variable.parameter.register.ic10"],
      "settings": { "foreground": "#0080FF" }
    },
    {
      "scope": ["variable.language.device.ic10"],
      "settings": { "foreground": "#00AA00" }
    },
    {
      "scope": ["constant.numeric.ic10"],
      "settings": { "foreground": "#20B2AA" }
    },
    {
      "scope": ["support.type.logic.ic10"],
      "settings": { "foreground": "#ff6516" }
    },
    {
      "scope": ["constant.language.enum.ic10"],
      "settings": { "foreground": "#20B2AA" }
    },
    {
      "scope": ["comment.line.number-sign.ic10"],
      "settings": { "foreground": "#808080", "fontStyle": "italic" }
    },
    {
      "scope": ["entity.name.label.ic10"],
      "settings": { "foreground": "#800080" }
    },
    {
      "scope": ["variable.other.ic10"],
      "settings": { "foreground": "#FFFFFF" }
    },
    {
      "scope": ["support.function.builtin.ic10"],
      "settings": { "foreground": "#808080" }
    }
  ]
}
